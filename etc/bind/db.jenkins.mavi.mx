;
; BIND data file for local loopback interface
;
$TTL	604800
@       IN      SOA     jenkins.mavi.mx. root.jenkins.mavi.mx. (
							  2         ; Serial
						 604800         ; Refresh
						  86400         ; Retry
						2419200         ; Expire
						 604800 )       ; Negative Cache TTL
;
@						IN	NS	jenkins.mavi.mx.
jenkins.mavi.mx.		IN	A	172.16.214.184
dns.jenkins.mavi.mx.	IN	A	172.16.214.184
pc1.jenkins.mavi.mx.	IN	A	172.16.214.184
www.jenkins.mavi.mx.	IN	A	172.16.214.184
