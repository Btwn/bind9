;
; BIND data file for local loopback interface
;
$TTL	604800
@       IN      SOA     registry.mavi.mx. root.registry.mavi.mx. (
							  2         ; Serial
						 604800         ; Refresh
						  86400         ; Retry
						2419200         ; Expire
						 604800 )       ; Negative Cache TTL
;
@					IN	NS	registry.mavi.mx.
registry.mavi.mx.		IN	A	172.16.214.184
dns.registry.mavi.mx.	IN	A	172.16.214.184
pc1.registry.mavi.mx.	IN	A	172.16.214.184
www.registry.mavi.mx.	IN	A	172.16.214.184
