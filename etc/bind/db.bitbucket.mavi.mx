;
; BIND data file for local loopback interface
;
$TTL	604800
@       IN      SOA     bitbucket.mavi.mx. root.bitbucket.mavi.mx. (
							  2         ; Serial
						 604800         ; Refresh
						  86400         ; Retry
						2419200         ; Expire
						 604800 )       ; Negative Cache TTL
;
@						IN	NS	bitbucket.mavi.mx.
bitbucket.mavi.mx.		IN	A	172.16.214.184
dns.bitbucket.mavi.mx.	IN	A	172.16.214.184
pc1.bitbucket.mavi.mx.	IN	A	172.16.214.184
www.bitbucket.mavi.mx.	IN	A	172.16.214.184
