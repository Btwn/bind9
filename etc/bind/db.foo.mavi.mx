;
; BIND data file for local loopback interface
;
$TTL	604800
@       IN      SOA     foo.mavi.mx. root.foo.mavi.mx. (
							  2         ; Serial
						 604800         ; Refresh
						  86400         ; Retry
						2419200         ; Expire
						 604800 )       ; Negative Cache TTL
;
@					IN	NS	foo.mavi.mx.
foo.mavi.mx.		IN	A	172.16.214.184
dns.foo.mavi.mx.	IN	A	172.16.214.184
pc1.foo.mavi.mx.	IN	A	172.16.214.184
www.foo.mavi.mx.	IN	A	172.16.214.184
